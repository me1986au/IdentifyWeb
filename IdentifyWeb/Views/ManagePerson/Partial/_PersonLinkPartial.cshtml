
@using IdentifyWeb.Models
@model IdentifyWeb.Models.PersonSectionViewModel
@{
    Layout = null; //Disables the footer from getting rendered
    ViewBag.Title = "Manage";
}
@{
    if (Model.PersonLinks != null)
    {
        foreach (var personLinkDto in Model.PersonLinks)
        {

            
            <div>
                @Html.Label(@personLinkDto.FirstName)
                @Html.Label(@personLinkDto.LastName)
                (@Html.Label(@personLinkDto.Alias))
                <a href=@personLinkDto.EditUrl>Edit</a> |
                <a href=@personLinkDto.DeleteUrl>Delete</a>
                <br/>
                <br />
            </div>
        }
    }
}